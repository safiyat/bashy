function fetch_mos_cert(){
  FUEL_SERVER=141.255.186.158
  CONTROLLER=node-2
  FUEL_CERT_PATH=/var/lib/fuel/keys/1/haproxy/public_haproxy.pem
  CONTROLLER_CERT_PATH=/var/lib/astute/haproxy/public_haproxy.pem
  INTERMEDIATE_PATH=/tmp/public_haproxy.pem
  OUTPUT_PATH=/Users/safiyat/VoerEirAB/openRCs/newhaproxy_$(date +%Y%m%d).pem
  ssh root@${FUEL_SERVER} "scp ${CONTROLLER}:${CONTROLLER_CERT_PATH} ${INTERMEDIATE_PATH}"
  scp root@"${FUEL_SERVER}:${INTERMEDIATE_PATH}" "${OUTPUT_PATH}"
  ssh root@"${FUEL_SERVER}" "rm ${INTERMEDIATE_PATH}"
  ln -sf ${OUTPUT_PATH} /Users/safiyat/VoerEirAB/openRCs/cacert.pem
}
